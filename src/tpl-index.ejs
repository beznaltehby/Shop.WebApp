<!doctype html>
<html>
    <head>

        <meta charset="utf-8">
        <title>БезНалТех</title>
        
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

        <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
        <link href="https://fonts.googleapis.com/css?family=Noto+Sans" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

        <% for(var i=0; i < htmlWebpackPlugin.files.css; i++) { %>
            <link href="<%= htmlWebpackPlugin.files.css[i] %>" rel="stylesheet">
        <% } %>

        <% for(var i=0; i < htmlWebpackPlugin.files.chunks; i++) { %>
            <script src="<%= htmlWebpackPlugin.files.chunks[i].entry %>" defer></script>
        <% } %>

        <style>
            .loading .page-loader {
                display: flex;
            }
            .page-loader {
                display: none;
                position: absolute;
                top: 0;
                right: 0;
                bottom: 0;
                left: 0;
                z-index: 3000;
                opacity: 0.75;
                background-color: #000000;
                justify-content: center;
                align-items: center;
                animation-duration: 0.2s;
                animation-name: pageLoaderInsert;
            }
            .page-loader.loaded {
                animation-duration: 0.5s;
                animation-name: pageLoaderRemove;
                opacity: 0;
            }
            .page-loader .loader {
                margin-top: 300px;
                opacity: 0.9;
            }
            @keyframes pageLoaderInsert {
                from {
                    opacity: 0;
                }
                to {
                    opacity: 0.75;
                }
            }
            @keyframes pageLoaderRemove {
                from {
                    opacity: 0.75;
                }
                to {
                    opacity: 0;
                }
            }
            .loader {
                width: 40px;
                height: 40px;
            }
            .circle {
                position: relative;
                width: 100%;
                height: 100%;
                border-radius: 50%;
                color: #ffffff;
            }
            .circle,
            .circle:before,
            .circle:after {
                animation: 1s infinite ease-in-out;
                -webkit-animation: 1s infinite ease-in-out;
            }
            .circle:before,
            .circle:after {
                content:"";
                width: 100%;
                height: 100%;
                border-radius: 50%;
                position: absolute;
                top: 0;
                left: 0;
            }
            .circle:before {
                border: 1px solid currentColor;
                box-sizing: border-box;
                opacity: 0;
                animation-name: circleAnimation;
                -webkit-animation-name: circleAnimation;
            }
            .circle:after {
                background-color: currentColor;
                -webkit-animation-name: filledCircleAnimation;
                animation-name: filledCircleAnimation;
            }
            @keyframes circleAnimation {
                0% {
                    opacity: 1;
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
                50% {
                    opacity: 0;
                    transform: scale(1.3);
                    -webkit-transform: scale(1.3);
                }
                100% {
                    opacity: 0;
                    transform: scale(1.3);
                    -webkit-transform: scale(1.3);
                }
            }
            @-webkit-keyframes circleAnimation {
                0% {
                    opacity: 1;
                    -webkit-transform: scale(1);
                }
                50% {
                    opacity: 0;
                    -webkit-transform: scale(1.3);
                }
                100% {
                    opacity: 0;
                    -webkit-transform: scale(1.3);
                }
            }
            @keyframes filledCircleAnimation {
                0% {
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
                50% {
                    transform:  scale(0.7);
                    -webkit-transform:  scale(0.7);
                }
                100% {
                    transform: scale(1);
                    -webkit-transform: scale(1);
                }
            }
            @-webkit-keyframes filledCircleAnimation {
                0% {
                    -webkit-transform: scale(1);
                }
                50% {
                    -webkit-transform:  scale(0.7);
                }
                100% {
                    -webkit-transform: scale(1);
                }
            }
        </style>

    </head>
    <body class="loading" page-loader>
        <div class="page-loader">
            <div class="loader">
                <div class="circle"></div>
            </div>
        </div>

        <main-header></main-header>
        <div ui-view></div>
    </body>
</html>
